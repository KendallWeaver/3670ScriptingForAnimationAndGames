global proc Rename(string $input[])
{
    string $selection[] = `ls -selection -flatten`;
    print $selection;

    for($i=0; $i<size($selection); $i++)
    {
        //print ($selection[$i] + " found!");
        int $numberSize = `tokenize $selection[$i] $input`;
        rename $input ("tail_" + $i + "_");
    }
}

Rename($input);